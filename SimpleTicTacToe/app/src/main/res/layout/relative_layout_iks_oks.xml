<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/cell_1_1"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"/>

    <Button
    android:id="@+id/cell_1_2"
    android:text="button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_toEndOf="@+id/cell_1_1"/>

    <Button
        android:id="@+id/cell_1_3"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/cell_1_2"/>

    <Button
        android:id="@+id/cell_2_1"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/cell_1_1"/>

    <Button
        android:id="@+id/cell_2_2"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/cell_2_1"
        android:layout_below="@+id/cell_1_2"/>

    <Button
        android:id="@+id/cell_2_3"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/cell_2_2"
        android:layout_below="@+id/cell_1_3"/>

    <Button
        android:id="@+id/cell_3_1"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/cell_2_1"/>

    <Button
        android:id="@+id/cell_3_2"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/cell_3_1"
        android:layout_below="@+id/cell_2_2"/>

    <Button
        android:id="@+id/cell_3_3"
        android:text="button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/cell_3_2"
        android:layout_below="@+id/cell_2_3"/>


</RelativeLayout>